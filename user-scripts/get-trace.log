#!/bin/bash

# arguments
# $1 servername
# $2 destination folder # needs to be writable by root (or other)

image="artifactory.eur.gad.schneider-electric.com:8888/ubuntu:focal-20220531"

DB_VOLUME="$1-db"
DB_FOLDER="/var/sbo"
DEST_FOLDER=$2

echo "Starting container..."
docker run --rm --mount type=bind,source=$DEST_FOLDER,target=/mnt --mount source=$DB_VOLUME,target=$DB_FOLDER $image bash -c "cp /var/sbo/db/logs/trace* /mnt/"

ls -l $DEST_FOLDER/trace*
